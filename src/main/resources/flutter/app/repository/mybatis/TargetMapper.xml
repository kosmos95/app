<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="flutter.app.repository.mybatis.TargetMapper">

    <select id="findName" resultType="target">
        select target_name, target_ganzi
        from target
        where target_id = #{id}
    </select>

</mapper>